<link href="~/css/modaladicionar.css" rel="stylesheet" />
<div class="modal modal-fixed-footer" id="modal">
    <div class="modal-content">
        <h6><strong>Adicionar uma intenção</strong></h6>
        <form class="col s12">
            <div class="row">
                <div class="input-field col s12">
                    <input id="intencao" type="text" class="validate" required>
                    <label for="intencao">Intenção</label>
                    <span class="helper-text" data-error="Informe uma intenção válida" data-success=""></span>
                </div>
                <div class="input-field col s12">
                    <input Placeholder="aa" id="especialidade" type="text" class="validate" disabled>
                    <label for="especialidade">Especialidade</label>
                </div>
            </div>
        </form>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn-small blue darken-4 modal-close">Cancelar</button>
        <button type="button" class="btn-small red darken-4" id="btnAdicionarIntencao">Adicionar</button>
    </div>
</div>
